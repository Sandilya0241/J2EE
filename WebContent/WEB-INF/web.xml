<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>JavaWebApplications</display-name>
  <welcome-file-list>
  	<welcome-file>Welcome.html</welcome-file>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>AadharHome.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <!-- servlet -->
  
	<servlet>
  		<servlet-name>FirstServlet</servlet-name>
  		<servlet-class>com.j2eelearnings.learnservlets.ServletExample</servlet-class>
  	</servlet>
  
	<servlet>
  		<servlet-name>SLC</servlet-name>
  		<servlet-class>com.j2eelearnings.learnservlets.ServletLifeCycleExample</servlet-class>
  		<load-on-startup>1</load-on-startup>
  	</servlet>
	
	<servlet>
  		<servlet-name>GenericServletName</servlet-name>
  		<servlet-class>com.j2eelearnings.learnservlets.GenericServletExample</servlet-class>
  	</servlet>
	
	<servlet>
		<description></description>
		<display-name>StudentRegistrationServlet</display-name>
  		<servlet-name>StudentRegistrationServlet</servlet-name>
  		<servlet-class>com.controller.StudentRegistrationServlet</servlet-class>
  		<init-param>
  			<description></description>
  			<param-name>username</param-name>
  			<param-value>employees_admin</param-value>
  		</init-param>
  		<init-param>
  			<description></description>
  			<param-name>password</param-name>
  			<param-value>employees_admin</param-value>
  		</init-param>
  	</servlet>
  	
  	<servlet>
		<description></description>
		<display-name>CourseRegistrationServlet</display-name>
  		<servlet-name>CourseRegistrationServlet</servlet-name>
  		<servlet-class>com.controller.CourseRegistrationServlet</servlet-class>
  		<init-param>
  			<description></description>
  			<param-name>username</param-name>
  			<param-value>emps</param-value>
  		</init-param>
  		<init-param>
  			<description></description>
  			<param-name>password</param-name>
  			<param-value>emps</param-value>
  		</init-param>
  	</servlet>
	
	<servlet>
		<description></description>
		<display-name>AadharServ</display-name>
  		<servlet-name>AadharServ</servlet-name>
  		<servlet-class>com.controller.AadharServlet</servlet-class>
  		<init-param>
  			<description></description>
  			<param-name>username</param-name>
  			<param-value>employees_admin</param-value>
  		</init-param>
  		<init-param>
  			<description></description>
  			<param-name>password</param-name>
  			<param-value>employees_admin</param-value>
  		</init-param>
  	</servlet>
  	
  	
  	<servlet>
		<servlet-name>ValidateServ</servlet-name>
  		<servlet-class>com.controller.ValServlet</servlet-class>
  	</servlet>
  	
  	<servlet>
		<servlet-name>RegisterServ</servlet-name>
  		<servlet-class>com.controller.RegisterServlet</servlet-class>
  		<init-param>
  			<description></description>
  			<param-name>username</param-name>
  			<param-value>employees_admin</param-value>
  		</init-param>
  		<init-param>
  			<description></description>
  			<param-name>password</param-name>
  			<param-value>employees_admin</param-value>
  		</init-param>
  	</servlet>

	<servlet>
		<servlet-name>GreetHelloServ</servlet-name>
  		<servlet-class>com.controller.GreetHelloServlet</servlet-class>
  	</servlet>
  	
  	<servlet>
		<servlet-name>ColorAppndrServ</servlet-name>
  		<servlet-class>com.controller.ColorAppndrServlet</servlet-class>
  	</servlet>
	
	<servlet>
		<servlet-name>RegServ</servlet-name>
  		<servlet-class>com.controller.RegServlet</servlet-class>
  	</servlet>
	
	
	
	<servlet>
		<servlet-name>CourseValidationServlet</servlet-name>
  		<servlet-class>com.controller.CourseValidationServlet</servlet-class>
  	</servlet>
	
	
	<servlet>
		<servlet-name>WelcomeServlet</servlet-name>
		<servlet-class>com.controller.Test2Servlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.controller.LogoutServlet</servlet-class>
	</servlet>
	
	
	
	<!-- servlet mapping -->
	
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	
	
	<servlet-mapping>
		<servlet-name>WelcomeServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	
	<servlet-mapping>
		<servlet-name>CourseValidationServlet</servlet-name>
  		<url-pattern>/CourseValidationServlet</url-pattern>
  	</servlet-mapping>
  	
	<servlet-mapping>
		<servlet-name>RegServ</servlet-name>
  		<url-pattern>/RegServ</url-pattern>
  	</servlet-mapping>
  	
	<servlet-mapping>
		<servlet-name>ColorAppndrServ</servlet-name>
  		<url-pattern>/ColorAppndrServ</url-pattern>
  	</servlet-mapping>
  	
  	<servlet-mapping>
		<servlet-name>GreetHelloServ</servlet-name>
  		<url-pattern>/GreetHelloServ</url-pattern>
  	</servlet-mapping>
	
	
	<servlet-mapping>
		<servlet-name>ValidateServ</servlet-name>
  		<url-pattern>/validate</url-pattern>
  	</servlet-mapping>
  	
  	<servlet-mapping>
		<servlet-name>RegisterServ</servlet-name>
  		<url-pattern>/register</url-pattern>
  	</servlet-mapping>
  	
  	
	<servlet-mapping>
		<servlet-name>FirstServlet</servlet-name>
  		<url-pattern>/Hello</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SLC</servlet-name>
  		<url-pattern>/HelpText</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>GenericServletName</servlet-name>
  		<url-pattern>/GenericServletExample</url-pattern>
	</servlet-mapping>
	
	
	<servlet-mapping>
		<servlet-name>StudentRegistrationServlet</servlet-name>
  		<url-pattern>/StudentRegistrationServlet</url-pattern>
  	</servlet-mapping>
  	
  	<servlet-mapping>
		<servlet-name>CourseRegistrationServlet</servlet-name>
  		<url-pattern>/CourseRegistrationServlet</url-pattern>
  	</servlet-mapping>
  	
  	<servlet-mapping>
		<servlet-name>AadharServ</servlet-name>
  		<url-pattern>/AadharServ</url-pattern>
	</servlet-mapping>
  	
  	<!-- Context Params -->
  	
	<context-param>
		<param-name>driver</param-name>
		<param-value>oracle.jdbc.OracleDriver</param-value>
	</context-param>
	
	<context-param>
		<param-name>url</param-name>
		<param-value>jdbc:oracle:thin:@localhost:1521:orclgdb</param-value>
	</context-param>


	<!-- Filters  -->
	
	<filter>
		<filter-name>CourseValidationFilter</filter-name>
		<filter-class>com.controller.CourseValidationFilter</filter-class>
		<init-param>
			<param-name>filterVal</param-name>
			<param-value>Demo Filter</param-value>
		</init-param>
  	</filter>
	
	<!-- Filter mapping -->
	
  	<filter-mapping>
		<filter-name>CourseValidationFilter</filter-name>
  		<url-pattern>/CourseValidationServlet</url-pattern>
  	</filter-mapping>


	<!-- Listeners -->
	<listener>
		<listener-class>com.controller.Test1</listener-class>
	</listener>
	
	<listener>
		<listener-class>com.controller.Test2</listener-class>
	</listener>
	
	<listener>
		<listener-class>com.controller.Test3</listener-class>
	</listener>
</web-app>