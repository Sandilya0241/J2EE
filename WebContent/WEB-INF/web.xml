<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>JavaWebApplications</display-name>
  <welcome-file-list>
    <welcome-file>GreetHello.html</welcome-file>
  </welcome-file-list>
  
  	<servlet>
		<servlet-name>GreetHelloServ</servlet-name>
  		<servlet-class>com.controller.GreetHelloServlet</servlet-class>
  	</servlet>
  	
  	<servlet>
		<servlet-name>ColorAppndrServ</servlet-name>
  		<servlet-class>com.controller.ColorAppndrServlet</servlet-class>
  	</servlet>
	
	
	
	
	<servlet-mapping>
		<servlet-name>ColorAppndrServ</servlet-name>
  		<url-pattern>/ColorAppndrServ</url-pattern>
  	</servlet-mapping>
  	
  	<servlet-mapping>
		<servlet-name>GreetHelloServ</servlet-name>
  		<url-pattern>/GreetHelloServ</url-pattern>
  	</servlet-mapping>
	
	
</web-app>